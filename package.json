    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-type-json": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-rc.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^11.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
