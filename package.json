{
  "name": "birdielab-api",
  "version": "1.0.0",
  "description": "Express server with TypeGraphQL and Supabase for golf profiles",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
